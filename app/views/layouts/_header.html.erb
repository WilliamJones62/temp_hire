<span class="primary-logo">
</span>
<span id="navbar">
  <% if user_signed_in? %>
    <span><%= link_to 'Sign out', destroy_user_session_path, method: :delete, class: "btn btn-primary" %></span>
  <% else %>
    <span><%= link_to 'Sign in', new_user_session_path, class: "btn btn-primary" %></span>
  <% end %>
</span>
<h1 class="title">Hours
</h1>
<% if user_signed_in? %>
  <span>Signed in as <%= current_user.email %></span>
<% else %>
  <span>No user currently signed in</span>
<% end %>
<%= content_tag(:div, flash[:error], :id => "flash_error") if flash[:error] %>
<%= content_tag(:div, flash[:notice], :id => "flash_notice") if flash[:notice] %>
<%= content_tag(:div, flash[:alert], :id => "flash_alert") if flash[:alert] %>

<br>
